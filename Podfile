# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
use_frameworks!

target 'AVPlayerDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  # 下载处理图片
  pod 'Kingfisher', '~> 5.7'
  # 布局约束
  pod 'SnapKit', '~> 5.0.0'
  # 弹框
  pod 'Toast-Swift', '~> 5.0.0'
  # 加载、解析动图
   pod 'lottie-ios'
   # 钥匙串访问
   pod 'SAMKeychain'
   # 网络请求
   pod 'Alamofire', '~> 4.8.2'

end
